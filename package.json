{
    "name": "odin-generatecode",
    "displayName": "odin-generatecode",
    "description": "vscode extensions",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/odinsam/odin-generatecode.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/odinsam/odin-generatecode/issues"
    },
    "homepage": "https://github.com/odinsam/odin-generatecode#readme",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:odin-generatecode.helloWorld"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "odin-generatecode.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "release": "standard-version",
        "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0",
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.60.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "eslint": "^7.27.0",
        "@typescript-eslint/eslint-plugin": "^4.26.0",
        "@typescript-eslint/parser": "^4.26.0",
        "glob": "^7.1.7",
        "mocha": "^8.4.0",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2",
        "better-than-before": "^1.0.0",
        "chai": "^4.2.0",
        "commitizen": "^3.1.1",
        "compare-func": "^1.3.2",
        "conventional-changelog-cli": "^2.0.21",
        "conventional-changelog-core": "^3.2.2",
        "coveralls": "^3.0.4",
        "cz-conventional-changelog": "^2.1.0",
        "git-dummy-commit": "^1.3.0",
        "github-url-from-git": "^1.5.0",
        "istanbul": "^0.4.5",
        "jscs": "^3.0.7",
        "jshint": "^2.10.2",
        "q": "^1.5.1",
        "shelljs": "^0.8.3",
        "standard-version": "^9.3.0",
        "through2": "^3.0.1"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    }
}
